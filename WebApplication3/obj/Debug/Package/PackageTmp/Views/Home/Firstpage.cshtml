
@{
    ViewBag.Title = "Firstpage";
    Layout = null;
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Christopher Jubell - My Game Dev Blog (Free materials)</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/site.css?v=@DateTime.Now">
    @Scripts.Render("~/bundles/modernizr")
    <link href="https://fonts.googleapis.com/css?family=Reenie+Beanie" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300" rel="stylesheet">
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Dev vlog", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>

            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Free Textures/models", "materials", "Home")</li>
                </ul>
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("HomePage", "Firstpage", "Home")</li>
                </ul>

                <ul class="nav navbar-nav hidden-xs" style="float:right">
                    <li><a>*13 free materials and counting!*</a></li>
                </ul>
            </div>


        </div>
    </div>






    <div class="fullscreen-bg">
        <video loop muted autoplay poster="img/videoframe.jpg" class="fullscreen-bg__video">
            <source src="http://christopherj-002-site1.gtempurl.com/images/preview_video.mp4" type="video/mp4">
            <source src="video/big_buck_bunny.ogv" type="video/ogg">
        </video>
    </div>


    <a href="@Url.Action("Materials","Home",new { id= 2})">
    <div style="
    margin: auto;
    text-align: center;
    color: ghostwhite;
    font-size: 1.4em;
    padding-top: 5vh;
    width: 300px;
"><div style="
    background-color: #811300;
    border-radius: 5px;
    border: 4px solid #431a00;
">Handpicked Free items:</div><br><img id="image-pickeditem" src="http://christopherj-002-site1.gtempurl.com/images/helm.png" style="
    border-radius: 50%;
    width: 300px;
    margin-top: 10px;
    border: 15px solid #b42e0d;
    height: 300px;
"></div></a>
    <div id="mytest" style="
    color: white;
    text-align: center;
    /* height: 300px; */
    /* padding: 400px; */
    top: 50%;
    position: absolute;
    width: 100%;
">
    
    <h1>My Game Developing Blog</h1><p> <span class="currentgame">Current game ~42% done</span><br />//Christopher Jubell</p></div>
    
    </body>
</html>

